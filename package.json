{
  "name": "Memory-Cards",
  "version": "1.0.0",
  "description": "Привет! Моя идея приложения - это флеш карты для запоминания английских слов. Приложение должно давать возможность создавать новые карточки, тренироваться в их запоминании и отслеживать прогресс.",
  "main": "index.js",
  "scripts": {
    "start": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all && node app.js",
    "dev": "nodemon app.js --ext js,jsx,json --ignore sessions",
    "dbr": "npx sequelize db:drop && npx sequelize db:create && npx sequelize db:migrate && npx sequelize db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kasatky/Memory-Cards.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kasatky/Memory-Cards/issues"
  },
  "homepage": "https://github.com/Kasatky/Memory-Cards#readme",
  "devDependencies": {
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sequelize": "^6.25.5",
    "sequelize-cli": "^6.5.2",
    "session-file-store": "^1.5.0",
    "sessions": "^0.0.2-7"
  }
}
